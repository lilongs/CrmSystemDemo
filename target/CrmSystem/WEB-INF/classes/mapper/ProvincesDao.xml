<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--指向Dao层-->
<mapper namespace="com.cn.dao.ProvincesDao">
    <select id="queryProvinces" resultType="Provinces" parameterType="Integer">
       SELECT temp as id,province as name FROM demo.provinces where temp=#{temp}
    </select>
</mapper>